{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~api/*": [
                "src/api/*"
            ],
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~constants/*": [
                "src/constants/*"
            ],
            "~containers/*": [
                "src/containers/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~redux/*": [
                "src/redux/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~Product/*": [
                "src/components/Product/*"
            ],
            "~Banner/*": [
                "src/containers/Banner/*"
            ],
            "~Footer/*": [
                "src/containers/Footer/*"
            ],
            "~Header/*": [
                "src/containers/Header/*"
            ],
            "~Products/*": [
                "src/containers/Products/*"
            ]
        }
    }
}